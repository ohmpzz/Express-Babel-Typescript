  "main": "main.ts",
  "scripts": {
    "babel-node": "npx babel-node src/main.ts --extensions \".ts,.tsx\" ",
    "dev": "nodemon -e * --watch src --exec yarn run babel-node",
    "build": "yarn run clean&&npx babel -d --out-dir build ./src --extensions \".ts,.tsx\"",
